<div>
    <form action="/authenticate" method="POST">
        <h1>Login:</h1>
        <label for="inputUsername">Username</label>
        <input class="form-control" type="username" name="username" id="inputusername" placeholder="Username" required><br>
        <br>
        <label for="inputPassword">Password</label>
        <input class="form-control" type="password" name="password" id="inputPassword" placeholder="Password" required><br>
        <br>
        <br><button type="submit" class="login-btn">Login</button>
    </form>
</div>